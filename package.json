    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babelify": "^7.3.0",
    "uglify-js": "^3.1.3",
    "webpack": "^3.6.0",
    "chai": "^3.5.0",
